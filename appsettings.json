{
    "AllowedHosts": "*",

    "Compression": {
        "Enable": true
    },

    "Middlewares": [],

    "Router": {
        "MaxConcurrentTotal": 65535,
        "MaxIdlePerRouter": 10,
        "MaxConcurrentPerRouter": 100,
        "MaxInvokePerInstance": 10000
    },

    "Cookie": {
        "Keys": "fastapi"
    },

    "Session": {
        "Enable": true,
        "Timeout": "20m",
        "Cookie": {
            "Name": "fastapi.sid",
            "SameSite": "Lax",
            "HttpOnly": true,
            "MaxAge": "20m"
        },
        "Redis": {
            "Enable": false
        }
    },

    "Cors": {
        "Enable": true,
        "Options": {
            "AllowMethods": [ "*" ],
            "AllowSites": [ "http://localhost:4000", "http://localhost:9527", "http://localhost:9528" ],
            "SupportsCredentials": true
        }
    }
}
