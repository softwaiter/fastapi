{
    "AllowedHosts": "*",

    "Middlewares": [],

    "Router": {
        "MaxConcurrentTotal": 65535,
        "MaxIdlePerRouter": 10,
        "MaxConcurrentPerRouter": 100,
        "MaxInvokePerInstance": 10000
    },

    "Cookie": {
        "Keys": "fastapi"
    },

    "Cors": {
        "Enable": true,
        "Options": {
            "AllowMethods": [ "*" ],
            "AllowSites": [ "http://localhost:4000", "http://localhost:9527" ],
            "SupportsCredentials": true //为true时，AllowSites不能为*
        }
    }
}
