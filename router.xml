<?xml version="1.0" encoding="utf-8" ?>
<routers>
    <router path="/hello" handler="CodeM.FastApi.Controllers.Hello.Handle"></router>
    <router path="/product" model="Product"></router>
    <router path="/module" model="Module"></router>
    <router path="/role" model="Role"></router>
    <router path="/role/module" model="RoleModule" batchAction="CR"></router>
    <router path="/org" model="Organization"></router>
    <router path="/org/product" model="OrgProduct"></router>
    <router path="/user" model="User"></router>
    <router path="/user/product" model="UserProduct"></router>
    <router path="/user/role" model="UserRole" batchAction="CR"></router>
    <router path="/proxy" model="Proxy"></router>
    <router path="/proxy/node" model="ProxyNode"></router>
    <router path="/proxy/reload" method="POST" handler="CodeM.FastApi.Controllers.ProxyReload.Handle" />
    <router path="/resetpass/{userid}" method="POST" handler="CodeM.FastApi.Controllers.ResetPass.Handle" />
</routers>