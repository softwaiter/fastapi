<?xml version="1.0" encoding="utf-8" ?>
<model name="PermissionData" table="t_permission_data">
    <property name="Id" field="f_id" notNull="true" primary="true" autoIncrement="true" joinInsert="false" jojnUpdate="false" desc="主键"/>
    <property name="Permission" field="f_permission_code" type="Permission" joinProp="Code" notNull="true" uniqueGroup="uc_permission_role" desc="关联权限" />
    <property name="Role" field="f_role_code" type="Role" joinProp="Code" notNull="true" uniqueGroup="uc_permission_role" desc="匹配角色" />
    <property name="Code" field="f_code" length="32" notNull="true" uniqueGroup="uc_code" joinUpdate="false" desc="数据权限编码" />"
    <property name="Actived" field="f_actived" type="Boolean" indexGroup="idx_actived" desc="是否激活" defaultValue="true" />
    <property name="Deleted" field="f_deleted" type="Boolean" indexGroup="idx_deleted" desc="是否删除" defaultValue="false" />
    <property name="CreateTime" type="DateTime" field="f_createtime" defaultValue="{{CurrentDateTime}}" joinUpdate="false" desc="创建时间" />
    <property name="UpdateTime" type="DateTime" field="f_updatetime" beforeSave="{{CurrentDateTime}}" desc="更新时间" />
</model>