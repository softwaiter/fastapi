<?xml version="1.0" encoding="utf-8" ?>
<model name="UserProduct" table="t_user_product">
    <property name="Id" field="f_id" notNull="True" primary="true" autoIncrement="true" joinInsert="false" jojnUpdate="false" desc="主键"/>
    <property name="UserCode" field="f_user_code" type="User" joinProp="Code" notNull="true" uniqueGroup="uc_user_prod" desc="所属用户编码" />
    <property name="ProductCode" field="f_prod_code" type="Product" joinProp="Code" notNull="true" uniqueGroup="uc_user_prod" desc="所属产品编码" />
    <property name="Expires" field="f_expires" type="DateTime" notNull="true" desc="有效期限" />
    <property name="State" field="f_state" type="Int16" desc="状态（0:正常，1:停用，2:删除）" defaultValue="0" />
    <property name="CreateTime" type="DateTime" field="f_createtime" defaultValue="{{CurrentDateTime}}" joinUpdate="false" desc="创建时间" />
    <property name="UpdateTime" type="DateTime" field="f_updatetime" beforeSave="{{CurrentDateTime}}" desc="更新时间" />
</model>